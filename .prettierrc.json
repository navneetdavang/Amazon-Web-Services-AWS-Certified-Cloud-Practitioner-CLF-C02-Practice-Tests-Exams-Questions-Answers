{
    "tabWidth": 2,
    "printWidth": 80,
    "arrowParens": "always",
    "proseWrap": "always",
    "parser": "typescript",
    "quoteProps": "as-needed",
    "singleQuote": true,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "useTabs": true,
    "trailingComma": "all",
    "singleAttributePerLine": true,
    "embeddedLanguageFormatting": "auto"
}